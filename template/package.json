{
  "name": "Blade",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "npx react-native start",
    "reset": "npx react-native start --reset-cache",
    "ios:dev": "npx react-native run-ios --simulator 'iPhone 14 Pro Max' --scheme 'BladeDEV'",
    "ios:stg": "npx react-native run-ios  --simulator 'iPhone 14 Pro Max' --scheme 'BladeSTG'",
    "ios:prod": "npx react-native run-ios  --simulator 'iPhone 14 Pro Max' --scheme 'Blade'",
    "android:dev": "npx react-native run-android --variant=developmentdebug",
    "android:stg": "npx react-native run-android --variant=stagingdebug",
    "android:prod": "npx react-native run-android --variant=productiondebug",
    "android:dev-release": "npx react-native run-android --variant=developmentrelease",
    "android:stg-release": "npx react-native run-android --variant=stagingrelease",
    "android:prod-release": "npx react-native run-android --variant=productionrelease",
    "test": "jest",
    "lint": "eslint .",
    "generate": "plop"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "axios": "0.27.2",
    "axios-retry": "^3.3.1",
    "deprecated-react-native-prop-types": "^2.3.0",
    "lodash": "^4.17.21",
    "lottie-react-native": "5.1.3",
    "react": "18.1.0",
    "react-native": "0.70.5",
    "react-native-config": "^1.4.11",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-keychain": "^8.1.1",
    "react-native-reanimated": "^2.13.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screen": "^1.0.1",
    "react-native-screens": "^3.18.2",
    "react-native-splash-screen": "^3.3.0",
    "react-native-vector-icons": "^9.2.0",
    "validator": "^13.7.0",
    "valtio": "^1.7.5",
    "valtio-persist": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "plop": "^3.1.1",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
